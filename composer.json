{
  "name": "da/intranetv3",
  "description": "Intranet, Version 3 de l'IUT de Troyes et de Reims Ch√¢lons Charleville.",
  "type": "project",
  "license": "MPL-2.0",
  "require": {
    "php": "^8.1",
    "ext-bcmath": "*",
    "ext-json": "*",
    "ext-zip": "*",
    "bugsnag/bugsnag-symfony": "^1.5",
    "composer/package-versions-deprecated": "1.11.99.3",
    "doctrine/annotations": "^1.0",
    "doctrine/doctrine-bundle": "^2.4",
    "doctrine/doctrine-migrations-bundle": "^3.1",
    "doctrine/orm": "^2.9",
    "endroid/qr-code-bundle": "^4.0",
    "erusev/parsedown": "^1.7",
    "friendsofsymfony/jsrouting-bundle": "^2.6",
    "jasig/phpcas": "^1.3",
    "knplabs/knp-snappy-bundle": "^1.8",
    "liip/imagine-bundle": "^2.3",
    "nesbot/carbon": "^2.37",
    "ninsuo/symfony-collection": "^2.1",
    "phpdocumentor/reflection-docblock": "^5.3",
    "phpoffice/phpspreadsheet": "^1.17",
    "phpoffice/phpword": "^0.18.1",
    "phpstan/phpdoc-parser": "^1.2",
    "ramsey/uuid-doctrine": "^1.6",
    "sensio/framework-extra-bundle": "^5.5",
    "symfony/asset": "^5.1",
    "symfony/console": "^5.1",
    "symfony/dotenv": "^5.1",
    "symfony/event-dispatcher": "^5.1",
    "symfony/expression-language": "^5.1",
    "symfony/flex": "^1.1",
    "symfony/form": "^5.1",
    "symfony/framework-bundle": "^5.1",
    "symfony/ldap": "^5.1",
    "symfony/mailer": "^5.1",
    "symfony/messenger": "^5.1",
    "symfony/mime": "^5.1",
    "symfony/monolog-bundle": "^3.5",
    "symfony/notifier": "^5.1",
    "symfony/property-access": "^5.1",
    "symfony/property-info": "^5.1",
    "symfony/proxy-manager-bridge": "^5.1",
    "symfony/routing": "^5.1",
    "symfony/runtime": "^5.1",
    "symfony/security-bundle": "^5.1",
    "symfony/serializer": "^5.1",
    "symfony/translation": "^5.1",
    "symfony/twig-bridge": "^5.1",
    "symfony/twig-bundle": "^5.1",
    "symfony/ux-chartjs": "^2.0",
    "symfony/ux-live-component": "^2.1",
    "symfony/ux-twig-component": "^2.0",
    "symfony/validator": "^5.1",
    "symfony/webpack-encore-bundle": "^1.7",
    "symfony/workflow": "^5.1",
    "symfony/yaml": "^5.1",
    "twig/extra-bundle": "^3.0",
    "twig/intl-extra": "^3.0",
    "twig/markdown-extra": "^3.3",
    "vich/uploader-bundle": "^1.8"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true,
    "optimize-autoloader": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "endroid/installer": true,
      "symfony/flex": true,
      "symfony/runtime": true
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "replace": {
    "paragonie/random_compat": "2.*",
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "^5.1"
    }
  },
  "require-dev": {
    "dealerdirect/phpcodesniffer-composer-installer": "^0.7",
    "doctrine/doctrine-fixtures-bundle": "^3.3",
    "nunomaduro/phpinsights": "^v2.0.0",
    "phpstan/phpstan": "^1.3",
    "phpstan/phpstan-doctrine": "^1.0",
    "phpstan/phpstan-symfony": "^1.0",
    "phpunit/phpunit": "^9.5",
    "rector/rector": "^0.12.10",
    "symfony/browser-kit": "^5.3",
    "symfony/css-selector": "^5.3",
    "symfony/maker-bundle": "^1.19",
    "symfony/phpunit-bridge": "^5.3",
    "symfony/web-profiler-bundle": "^5.1"
  }
}
