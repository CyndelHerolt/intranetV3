{% extends 'base.html.twig' %}

{% block header %}
    {{ 'adm.apc_niveau.show.heading'|trans }}
{% endblock %}

{% block headeractions %}
    <div class="header-action">
        {{ include('composants/adm-buttons-show.html.twig', {elt: 'apc_niveau', id: apc_niveau.id}) }}
    </div>
{% endblock %}

{% block content %}
    <div class="card">
        <h4 class="card-title">{{ 'adm.apc_niveau.show.heading'|trans }}</h4>
        <div class="card-body">

            <table class="table">
                <tbody>
                <tr>
                    <th>Id</th>
                    <td>{{ apc_niveau.id }}</td>
                </tr>
                <tr>
                    <th>Libelle</th>
                    <td>{{ apc_niveau.libelle }}</td>
                </tr>
                <tr>
                    <th>Ordre</th>
                    <td>{{ apc_niveau.ordre }}</td>
                </tr>
                </tbody>
            </table>
        </div>
    </div>
{% endblock %}
