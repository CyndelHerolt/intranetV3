{% extends 'base.html.twig' %}

{% block header %}
    {{ 'adm.personnel.index.heading'|trans }}
{% endblock %}

{% block content %}
    <div class="card">
        <h4 class="card-title">{{ 'adm.personnel.index.heading'|trans }}</h4>
        <div class="card-body">
            {{ table_render(table) }}
        </div>
    </div>
{% endblock %}

{% block fabactions %}
    {{ include('composants/fab-button-help.html.twig', {
        href:path('help_article', {sujet: 'administration-personnel-index'})}
    ) }}
{% endblock %}

{% block stylesheets %}
    {{ parent() }}
    {{ encore_entry_link_tags('adm.personnels') }}
{% endblock %}

{% block javascripts %}
    {{ parent() }}
    {{ encore_entry_script_tags('adm.personnels') }}
{% endblock %}
