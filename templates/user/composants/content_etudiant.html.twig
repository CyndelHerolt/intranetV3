{% if is_granted('ROLE_PERMANENT') or user.id == user_data.user.id %}
    {% if onglet == 'absences' %}
        {{ render(controller(
            'App\\Controller\\ProfilEtudiantController::absences', {slug: user.slug}
        )) }}
    {% elseif onglet == 'notes' %}
        {{ render(controller(
            'App\\Controller\\ProfilEtudiantController::absences', {slug: user.slug}
        )) }}
    {% elseif onglet == 'apc' and (is_granted('ROLE_PERMANENT')  or (is_granted('ROLE_ETUDIANT') and user.semestre
    .optBilanSemestre == true) %}
        {{ render(controller(
            'App\\Controller\\ProfilEtudiantController::apcNotes', {slug: user.slug}
        )) }}
    {% elseif onglet == 'stages' %}
        {{ render(controller(
            'App\\Controller\\ProfilEtudiantController::stage', {slug: user.slug}
        )) }}
    {% elseif onglet == 'scolarite' %}
        {{ render(controller(
            'App\\Controller\\ProfilEtudiantController::scolarite', {slug: user.slug}
        )) }}
    {% else %}
        {{ render(controller(
            'App\\Controller\\ProfilEtudiantController::scolarite', {slug: user.slug}
        )) }}
