<h3>Demande du {{ emprunt.created ? emprunt.created|format_datetime('long', 'short') : '' }}</h3>

<div class="row">

    <div class="col-sm-12">
        <p>{{ emprunt.dateDebut|format_datetime('short', 'short' ) }} | {{ emprunt.datefin|format_datetime('short',
                'short') }}</p>
        <p>Motif : {{ emprunt.motif }}</p>
        <p>Description : {{ emprunt.description }}</p>
        <ul>
            {% for materiel in emprunt.empruntMateriels %}
                <li>{{ materiel.materiel.libelle }}</li>
            {% endfor %}
        </ul>
    </div>
</div>
