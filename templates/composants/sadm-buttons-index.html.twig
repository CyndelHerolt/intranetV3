<div class="buttons">
    {% if params is not defined %}
        {% set params = {} %}
    {% endif %}
    <a href="{{ path('sa_'~ elt ~ '_new', params) }}" class="btn btn-info btn-sm btn-float" title="{{ 'atitle
    .creer_nouveau'|trans }}" data-bs-toggle="tooltip" data-bs-placement="bottom"><i class="{{ iconAdd }}"></i></a>
    {% if retour is defined %}
        <a href="{{ path(retour.path) }}"
           class="btn btn-primary btn-float"
           data-bs-toggle="tooltip" data-bs-placement="bottom"

           title="{{ retour.titre }}">
            <i class="{{ iconRetour }}"></i>
            <span class="sr-only">{{ retour.titre }}</span>
        </a>
    {% else %}
        <a href="{{ path('super_admin_homepage') }}" class="btn btn-primary btn-float" data-bs-toggle="tooltip" data-bs-placement="bottom"
           title="{{ 'atitle.retour_administration'|trans }}">
            <i class="{{ iconDashboard }}"><span class="sr-only">dashboard</span></i>
        </a>
    {% endif %}

</div>
